<?xml version="1.0"?>
<Container version="2">
  <Name>lobe-chat</Name>
  <Repository>lobehub/lobe-chat</Repository>
  <Registry>https://hub.docker.com/r/lobehub/lobe-chat</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/lobehub/lobe-chat/issues</Support>
  <Project>https://github.com/lobehub/lobe-chat</Project>
  <Overview>LobeChat is an open-source, extensible (Function Calling) high-performance chatbot framework.&#xD;
It supports one-click free deployment of your private ChatGPT/LLM web application.&#xD;
&#xD;
https://github.com/lobehub/lobe-chat/wiki&#xD;
&#xD;
If you need to use the OpenAI service through a proxy, you can configure the proxy address using the OPENAI_PROXY_URL environment variable:&#xD;
OPENAI_PROXY_URL=https://api-proxy.com/v1</Overview>
  <Category>Network:Web</Category>
  <WebUI>http://[IP]:[PORT:3210]</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/Joly0/docker-templates/main/templates/lobe-chat.xml</TemplateURL>
  <Icon>https://camo.githubusercontent.com/7b369a5fc076ac669c7bc05baa9d8492d61ce0d6610cc613cd70746bdb09493e/68747470733a2f2f72656769737472792e6e706d6d6972726f722e636f6d2f406c6f62656875622f6173736574732d6c6f676f2f312e302e302f66696c65732f6173736574732f6c6f676f2d33642e77656270</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DonateText/>
  <DonateLink/>
  <Requires>Requires an OpenAI API Key</Requires>
  <Config Name="WebUI" Target="3210" Default="3210" Mode="tcp" Description="" Type="Port" Display="always" Required="false" Mask="false">3210</Config>
  <Config Name="OPENAI API Key" Target="OPENAI_API_KEY" Default="" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="true"></Config>
  <Config Name="Access Code" Target="ACCESS_CODE" Default="lobe66" Mode="" Description="The password set in the official Docker image is lobe66 by default. Replace it with your own password to improve security." Type="Variable" Display="always" Required="false" Mask="false">lobe66</Config>
</Container>